<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<!-- xml 이름은 (사용하고자 하는) Interface 이름과 동일하게 만든다. -->
  
  				<!-- TimeMapper 인터페이스의 FQCN으로 입력 -->
<mapper namespace="org.zerock.myapp.mapper.OtherMapper">  
			
			<!-- 사용하고자 하는 메소드 이름과 동일 -->
	<select id="getNow2" >
		SELECT current_date FROM dual
	</select>
</mapper>
